{
  "version": "0.0.1-beta.0",
  "dependencies": {
    "@types/graphql": "^14.0.5",
    "@types/recompose": "^0.30.3",
    "@vtex/address-form": "^3.0.2",
    "@vtex/delivery-packages": "^2.16.2",
    "@vtex/estimate-calculator": "^1.1.0",
    "apollo-boost": "^0.1.22",
    "apollo-client": "^2.4.7",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.10.1",
    "react": "^16.6.3",
    "react-apollo": "^2.3.2",
    "react-clipboard.js": "^2.0.2",
    "react-intl": "^2.7.2",
    "recompose": "^0.30.0"
  },
  "devDependencies": {
    "@types/jest": "^23.3.13",
    "@types/node": "^10.12.21",
    "@types/prop-types": "^15.5.8",
    "@types/react": "^16.8.1",
    "@types/react-intl": "^2.3.15",
    "jest": "^23.6.0",
    "jest-dom": "^3.0.0",
    "jest-transform-graphql": "^2.1.0",
    "prettier": "^1.16.1",
    "react-dom": "^16.7.0",
    "react-testing-library": "^5.4.4",
    "regenerator-runtime": "^0.13.1",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.1",
    "tslint-config-vtex": "^2.1.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.3.1"
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "tsc --noEmit && tslint -c tslint.json './**/*.ts'",
    "lint:locales": "intl-equalizer"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|scss)$": "<rootDir>/__mocks__/styleMock.ts"
    },
    "setupTestFrameworkScriptFile": "./testUtils/setupTests.ts",
    "collectCoverageFrom": [
      "react/**/*.{ts, tsx}"
    ],
    "transform": {
      "\\.(gql|graphql)$": "jest-transform-graphql",
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node",
      "graphql"
    ]
  }
}
