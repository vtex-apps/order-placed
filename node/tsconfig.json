{
  "compilerOptions": {
    "types": ["node"],
    "typeRoots": ["node_modules/@types"],
    "lib": ["esnext.asynciterable", "es2017"],
    "target": "es2017",
    "module": "commonjs",
    "sourceMap": false,
    "allowJs": true
  },
  "typeAcquisition": {
    "enable": false
  },
  "include": ["./**/*"],
  "exclude": ["node_modules"],
  "compileOnSave": true
}
